<tr>
	<td><%= establishment_service.name %></td>
	<td><%= establishment_service.establishment.name %></td>
	<% if has_permission(controller_path,"update") || has_permission(controller_path,"destroy") %>
		<td>
			<% if has_permission(controller_path,"update") %>
				<%= link_to edit_admin_establishment_service_path(establishment_service) do %><i class="fa fa-edit"></i><% end %>
			<% end %>
			<% if has_permission(controller_path,"destroy") %>
				<%= link_to admin_establishment_service_path(establishment_service), data: { confirm: "Are you sure ?" }, method: :delete do %><i class="fa fa-trash"></i><% end %>
			<% end %>
		</td>
	<% end %>
</tr>